{
    "name": "lua-eclipse",
    "displayName": "Lua Eclipse Theme",
    "description": "A dark pastel theme for Lua with JSON support.",
    "publisher": "emilrueh",
    "version": "0.1.1",
    "license": "MIT",
    "icon": "icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/emilrueh/lua-vsc-theme"
    },
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Lua Eclipse",
                "uiTheme": "vs-dark",
                "path": "./themes/lua-eclipse-color-theme.json"
            }
        ],
        "configurationDefaults": {
            "[lua]": {
                "editor.semanticHighlighting.enabled": false
            }
        }
    },
    "keywords": [
        "lua",
        "love",
        "löve",
        "love2d",
        "lovr",
        "lövr",
        "theme",
        "dark",
        "night"
    ]
}